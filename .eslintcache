the reducer\nimport {\n    SEARCH_USERS,\n    SET_LOADING,\n    CLEAR_USERS,\n    GET_USER,\n    GET_REPOS\n\n} from '../types';\n\nexport default (state, action)=>{\n    switch(action.type)\n    {\n        case SEARCH_USERS:\n            return {\n                ...state,\n                users: action.payload,\n                loading: false\n            };\n        case CLEAR_USERS:\n            return {\n                ...state,\n                users: [],\n                loading: false\n            };\n        case GET_REPOS:\n            return {\n                ...state,\n                repos: action.payload,\n                loading: false\n            };\n        case GET_USER:\n            return {\n                ...state,\n                user: action.payload,\n                loading: false\n            };\n        case SET_LOADING:\n            //we have to return whatever is already in the state, state is immutable and we cant re-assign it \n            //we have to make a copy of it and make any additions or changes to it\n            // the spread operator \"...\" is used to make a copy\n            return {\n                ...state,\n                loading: true\n            };\n        default:\n            return state;\n        \n    }\n}","/home/mythreyi/Desktop/udemy/react/github-finder/src/context/alert/AlertState.js",[],"/home/mythreyi/Desktop/udemy/react/github-finder/src/context/alert/alertContext.js",[],"/home/mythreyi/Desktop/udemy/react/github-finder/src/context/alert/alertReducer.js",["99"],"import { SET_ALERT, REMOVE_ALERT } from '../types';\n\nexport default (state, action) => {\n    switch (action.type) {\n        case SET_ALERT:\n            return action.payload;\n        case REMOVE_ALERT:\n            return null;\n        default:\n            return state;\n    }\n}","/home/mythreyi/Desktop/udemy/react/github-finder/src/components/pages/Home.js",[],"/home/mythreyi/Desktop/udemy/react/github-finder/src/components/pages/